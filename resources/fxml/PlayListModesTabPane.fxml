<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root prefHeight="358.0" prefWidth="811.0" stylesheets="@../style/application.css" type="StackPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane minHeight="0.0">
         <center>
            <JFXTabPane minHeight="0.0" styleClass="specialJFXTabPane" BorderPane.alignment="CENTER">
               <tabs>
                  <Tab fx:id="onlineMusicTab" closable="false" text="Online Music">
                   <graphic>
                        <FontIcon iconColor="#d03030" iconLiteral="fab-youtube" iconSize="20" />
                     </graphic>
                  </Tab>
                  <Tab fx:id="openedLibrariesTab" closable="false" text="Opened Libraries">
                   <graphic>
                        <FontIcon iconColor="#dba637" iconLiteral="fas-folder-open" iconSize="20" />
                     </graphic>
                  </Tab>
                  <Tab fx:id="dropBoxTab" closable="false" text="Dropbox">
                   <graphic>
                        <FontIcon iconColor="#25c1ff" iconLiteral="fa-dropbox" iconSize="20" />
                     </graphic>
                  </Tab>
                  <Tab fx:id="emotionListsTab" closable="false" text="Emotion Lists">
                     <graphic>
                        <FontIcon iconColor="#ff3c26" iconLiteral="fas-heart" iconSize="20" />
                     </graphic>
                  </Tab>
                  <Tab fx:id="downloadsTab" closable="false" disable="true" text="Downloads ( Coming... )">
                     <content>
                        <StackPane prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <JFXTabPane minHeight="0.0" side="RIGHT" styleClass="specialJFXTabPane">
                                 <tabs>
                                    <Tab fx:id="dropBoxTab1" closable="false" text="Dropbox ">
                                       <graphic>
                                          <FontIcon iconColor="#25c1ff" iconLiteral="fa-dropbox" iconSize="20" />
                                       </graphic>
                                    </Tab>
                                 </tabs>
                              </JFXTabPane>
                           </children></StackPane>
                     </content>
                     <graphic>
                        <FontIcon iconColor="#ceff26" iconLiteral="fas-cloud-download-alt" iconSize="20" />
                     </graphic>
                  </Tab>
               </tabs>
            </JFXTabPane>
         </center>
      </BorderPane>
   </children>
</fx:root>
