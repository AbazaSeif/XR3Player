<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<fx:root stylesheets="@../style/application.css" type="StackPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox styleClass="online-music-vbox-box">
         <children>
            <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="descriptionLabel" alignment="CENTER" maxWidth="200.0" styleClass="online-music-vbox-box-label" text="Top Songs" textAlignment="CENTER" wrapText="true" />
         </children>
      </VBox>
      <Label fx:id="stackLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="online-music-vbox-box-stack-label" text="Search" textAlignment="CENTER" visible="false" wrapText="true" />
   </children>
</fx:root>
